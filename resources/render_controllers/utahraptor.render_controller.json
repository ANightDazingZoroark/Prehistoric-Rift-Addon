{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.utahraptor": {
			"geometry": "Geometry.default",
			"arrays": {
				"textures": {
					"Array.skins": [
						"Texture.variant1",
						"Texture.variant2",
						"Texture.variant3",
						"Texture.variant4"
					]
				}
			},
			"textures": [
				"Array.skins[query.variant]"
			],
			"part_visibility": [
				{
					"saddle*": "query.is_saddled",
					"chest*": "query.is_chested",
					"headSaddle*": "query.is_saddled",
					"upperJawHalloween*": "(query.variant) > 3 && (query.variant < 8)",
					"headFeathers*": "query.variant <= 3 || query.variant >= 8",
					"leftElbowWing*": "query.variant < 8",
					"leftArmWing*": "query.variant < 8",
					"rightElbowWing*": "query.variant < 8",
					"rightArmWing*": "query.variant < 8",
					"christmasHat*": "query.variant >= 8"
				}
			]
		},
		"controller.render.utahraptor_costumes": {
			"geometry": "Geometry.default",
			"arrays": {
				"textures": {
					"Array.skins": [
						"Texture.halloweenVariant",
						"Texture.christmasVariant"
					]
				}
			},
			"textures": [
				"(query.variant > 3) && (query.variant <= 7) ? Array.skins[0] : Array.skins[1]"
			],
			"part_visibility": [
				{
					"saddle*": "query.is_saddled",
					"chest*": "query.is_chested",
					"headSaddle*": "query.is_saddled",
					"upperJawHalloween*": "(query.variant) > 3 && (query.variant < 8)",
					"headFeathers*": "query.variant <= 3 || query.variant >= 8",
					"leftElbowWing*": "query.variant < 8",
					"leftArmWing*": "query.variant < 8",
					"rightElbowWing*": "query.variant < 8",
					"rightArmWing*": "query.variant < 8",
					"christmasHat*": "query.variant >= 8"
				}
			]
		}
	}
}