{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.direwolf": {
			"arrays": {
				"textures": {
					"Array.skinsNormal": [
						"Texture.variantNormal1",
						"Texture.variantNormal2",
						"Texture.variantNormal3",
						"Texture.variantNormal4"
					],
					"Array.skinsWerewolf": [
						"Texture.variantWerewolf1",
						"Texture.variantWerewolf2",
						"Texture.variantWerewolf3",
						"Texture.variantWerewolf4"
					]
				}
			},
			"geometry": "query.is_ignited ? Geometry.werewolf : Geometry.default",
			"textures": [
				"query.is_ignited ? Array.skinsWerewolf[query.variant] : Array.skinsNormal[query.variant]"
			]
		}
	}
}