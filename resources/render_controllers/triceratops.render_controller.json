{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.triceratops": {
			"geometry": "Geometry.default",
			"arrays": {
				"textures": {
					"Array.skins": [
						"Texture.variant1",
						"Texture.variant2",
						"Texture.variant3",
						"Texture.variant4",
						"Texture.halloweenVariant"
					]
				}
			},
			"textures": [
				"query.variant <= 3 ? Array.skins[query.variant] : Array.skins[4]"
			],
			"part_visibility": [
				{
					"saddle*": "query.is_saddled",
					"chest*": "query.is_chested",
					"beakSaddle*": "query.is_saddled"
				}
			]
		}
	}
}