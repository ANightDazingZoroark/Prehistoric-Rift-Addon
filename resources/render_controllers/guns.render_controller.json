{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.shotgun_first_person": {
			"geometry": "Geometry.firstPerson",
			"textures": [
				"Texture.default"
			]
		},
		"controller.render.shotgun_third_person": {
			"geometry": "Geometry.thirdPerson",
			"textures": [
				"Texture.default"
			]
		},
		"controller.render.rocket_launcher_first_person": {
			"geometry": "query.remaining_durability == 1 ? Geometry.firstPersonFull : Geometry.firstPersonEmpty",
			"arrays": {
				"textures": {
					"Array.textures": [
						"Texture.empty",
						"Texture.full"
					]
				}
			},
			"textures": [
				"Array.textures[query.remaining_durability]"
			]
		},
		"controller.render.rocket_launcher_third_person": {
			"geometry": "query.remaining_durability == 1 ? Geometry.thirdPersonFull : Geometry.thirdPersonEmpty",
			"arrays": {
				"textures": {
					"Array.textures": [
						"Texture.empty",
						"Texture.full"
					]
				}
			},
			"textures": [
				"Array.textures[query.remaining_durability]"
			]
		}
	}
}