{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.dodo": {
			"geometry": "Geometry.default",
			"arrays": {
				"textures": {
					"Array.skins": [
						"Texture.variant1",
						"Texture.variant2",
						"Texture.variant3",
						"Texture.variant4"
					]
				}
			},
			"textures": [
				"Array.skins[query.variant]"
			],
			"part_visibility": [
				{
					"head*": "query.variant <= 7",
					"upperBeak0*": "query.variant <= 7",
					"upperBeak1*": "query.variant <= 7",
					"lowerBeak*": "query.variant <= 7",
					"leftWingNormal*": "query.variant <= 7",
					"rightWingNormal*": "query.variant <= 7",
					"leftWingChristmas*": "query.variant > 7",
					"rightWingChristmas*": "query.variant > 7",
					"tail*": "query.variant <= 7",
					"leftToes*": "query.variant <= 7",
					"rightToes*": "query.variant <= 7"
				}
			]
		},
		"controller.render.dodo_costumes": {
			"geometry": "Geometry.default",
			"arrays": {
				"textures": {
					"Array.skins": [
						"Texture.halloweenVariant",
						"Texture.christmasVariant"
					]
				}
			},
			"textures": [
				"(query.variant > 3) && (query.variant <= 7) ? Array.skins[0] : Array.skins[1]"
			],
			"part_visibility": [
				{
					"head*": "query.variant <= 7",
					"upperBeak0*": "query.variant <= 7",
					"upperBeak1*": "query.variant <= 7",
					"lowerBeak*": "query.variant <= 7",
					"leftWingNormal*": "query.variant <= 7",
					"rightWingNormal*": "query.variant <= 7",
					"leftWingChristmas*": "query.variant > 7",
					"rightWingChristmas*": "query.variant > 7",
					"tail*": "query.variant <= 7",
					"leftToes*": "query.variant <= 7",
					"rightToes*": "query.variant <= 7"
				}
			]
		}
	}
}