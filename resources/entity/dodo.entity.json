{
	"format_version": "1.10.0",
	"minecraft:client_entity": {
		"description": {
			"identifier": "rift:dodo",
			"materials": {
				"default": "entity"
			},
			"textures": {
				"variant1": "textures/entity/dodo/dodo 1",
				"variant2": "textures/entity/dodo/dodo 2",
				"variant3": "textures/entity/dodo/dodo 3",
				"variant4": "textures/entity/dodo/dodo 4",
				"halloweenVariant": "textures/entity/dodo/dodo halloween",
				"christmasVariant": "textures/entity/dodo/dodo christmas"
			},
			"animations": {
				"animStanding": "animation.dodo.standing",
				"animSitting": "animation.dodo.sitting",
				"animWalk": "animation.dodo.walk",
				"animFall": "animation.dodo.falling",
				"animLook": "animation.dodo.look_at_target",
				"contMovement": "controller.animation.dodo_movement"
			},
			"scripts": {
				"animate": [
					"contMovement",
					"animLook"
				]
			},
			"geometry": {
				"default": "geometry.dodo"
			},
			"render_controllers": [
				{
					"controller.render.dodo": "query.variant <= 3"
				},
				{
					"controller.render.dodo_costumes": "query.variant > 3"
				}
			],
			"spawn_egg": {
				"base_color": "#777575",
				"overlay_color": "#5e5c5c"
			}
		}
	}
}