{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.taxidermy_stand": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"stegosaurus": "query.mark_variant >= 1 && query.mark_variant <= 4"
						},
						{
							"tyrannosaurus": "query.mark_variant >= 5 && query.mark_variant <= 8"
						},
						{
							"dodo": "query.mark_variant >= 9 && query.mark_variant <= 12"
						},
						{
							"triceratops": "query.mark_variant >= 13 && query.mark_variant <= 16"
						},
						{
							"utahraptor": "query.mark_variant >= 17 && query.mark_variant <= 20"
						}
					]
				},
				"stegosaurus": {
					"animations": [
						"animStego"
					],
					"transitions": [
						{
							"default": "query.mark_variant < 1 && query.mark_variant > 4"
						}
					]
				},
				"tyrannosaurus": {
					"animations": [
						"animTyranno"
					],
					"transitions": [
						{
							"default": "query.mark_variant < 5 && query.mark_variant > 8"
						}
					]
				},
				"dodo": {
					"animations": [
						"animDodo"
					],
					"transitions": [
						{
							"default": "query.mark_variant < 9 && query.mark_variant > 12"
						}
					]
				},
				"triceratops": {
					"animations": [
						"animTrike"
					],
					"transitions": [
						{
							"default": "query.mark_variant < 13 && query.mark_variant > 16"
						}
					]
				},
				"utahraptor": {
					"animations": [
						"animUtah"
					],
					"transitions": [
						{
							"default": "query.mark_variant < 17 && query.mark_variant > 20"
						}
					]
				}
			}
		},
		"controller.animation.taxidermy_stand_pose": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"stegosaurus": "query.mark_variant >= 1 && query.mark_variant <= 4"
						},
						{
							"tyrannosaurus": "query.mark_variant >= 5 && query.mark_variant <= 8"
						},
						{
							"dodo": "query.mark_variant >= 9 && query.mark_variant <= 12"
						},
						{
							"triceratops": "query.mark_variant >= 13 && query.mark_variant <= 16"
						},
						{
							"utahraptor": "query.mark_variant >= 17 && query.mark_variant <= 20"
						}
					]
				},
				"stegosaurus": {
					"animations": [
						{
							"animStegoPoseOne": "query.skin_id == 1"
						},
						{
							"animStegoPoseTwo": "query.skin_id == 2"
						},
						{
							"animStegoPoseThree": "query.skin_id == 3"
						}
					],
					"transitions": [
						{
							"default": "query.mark_variant < 1 && query.mark_variant > 4"
						}
					]
				},
				"tyrannosaurus": {
					"animations": [
						{
							"animTyrannoPoseOne": "query.skin_id == 1"
						},
						{
							"animTyrannoPoseTwo": "query.skin_id == 2"
						},
						{
							"animTyrannoPoseThree": "query.skin_id == 3"
						}
					],
					"transitions": [
						{
							"default": "query.mark_variant < 5 && query.mark_variant > 8"
						}
					]
				},
				"dodo": {
					"animations": [
						{
							"animDodoPoseOne": "query.skin_id == 1"
						},
						{
							"animDodoPoseTwo": "query.skin_id == 2"
						},
						{
							"animDodoPoseThree": "query.skin_id == 3"
						}
					],
					"transitions": [
						{
							"default": "query.mark_variant < 9 && query.mark_variant > 12"
						}
					]
				},
				"triceratops": {
					"animations": [
						{
							"animTrikePoseOne": "query.skin_id == 1"
						},
						{
							"animTrikePoseTwo": "query.skin_id == 2"
						},
						{
							"animTrikePoseThree": "query.skin_id == 3"
						}
					],
					"transitions": [
						{
							"default": "query.mark_variant < 13 && query.mark_variant > 16"
						}
					]
				},
				"utahraptor": {
					"animations": [
						{
							"animUtahPoseOne": "query.skin_id == 1"
						},
						{
							"animUtahPoseTwo": "query.skin_id == 2"
						},
						{
							"animUtahPoseThree": "query.skin_id == 3"
						}
					],
					"transitions": [
						{
							"default": "query.mark_variant < 17 && query.mark_variant > 20"
						}
					]
				}
			}
		}
    }
}