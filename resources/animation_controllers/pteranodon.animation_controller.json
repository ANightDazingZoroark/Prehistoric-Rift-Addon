{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.pteranodon_movement": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
                        "animLandPose",
						{
							"animWalk": "query.ground_speed > 0"
						}
					],
					"transitions": [
						{
							"flying": "!query.is_on_ground"
						}
					]
				},
				"flying": {
					"animations": [
						"animFlyPose",
                        "animFly"
					],
					"transitions": [
						{
							"default": "query.is_on_ground"
						}
					]
				}
			}
		}
    }
}