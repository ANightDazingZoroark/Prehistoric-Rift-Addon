{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.tyrannosaurus_movement": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						{
							"animStanding": "query.ground_speed == 0"
						},
						{
							"animWalk": "query.ground_speed > 0"
						}
					],
					"transitions": [
						{
							"sitting": "query.is_sitting"
						},
						{
							"sleeping": "query.is_sleeping"
						}
					]
				},
				"sitting": {
					"animations": [
						"animSitting"
					],
					"transitions": [
						{
							"default": "!query.is_sitting"
						}
					]
				},
				"sleeping": {
					"animations": [
						"animSleeping"
					],
					"transitions": [
						{
							"default": "!query.is_sleeping"
						}
					]
				}
			}
		},
		"controller.animation.tyrannosaurus_attack": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"attack": "query.is_delayed_attacking"
						}
					]
				},
				"attack": {
					"animations": [
						"animAttack"
					],
					"transitions": [
						{
							"default": "!query.is_delayed_attacking"
						}
					]
				}
			}
		}
	}
}