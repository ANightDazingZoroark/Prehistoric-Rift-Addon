{
    "format_version": "1.10.0",
    "animation_controllers": {
        "controller.animation.coelacanth_movement": {
            "initial_state": "default",
            "states": {
                "default": {
                    "animations": [
                        "animMove"
                    ],
                    "transitions": [
                        {
                            "flopping": "!query.is_in_water || query.is_levitating"
                        }
                    ]
                },
                "flopping": {
                    "animations": [
                        "animFlop"
                    ],
                    "transitions": [
                        {
                            "default": "query.is_in_water && !query.is_levitating"
                        }
                    ]
                }
            }
        }
    }
}