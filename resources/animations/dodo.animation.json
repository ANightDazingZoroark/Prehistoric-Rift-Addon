{
	"format_version": "1.8.0",
	"animations": {
		"animation.dodo.look": {
			"loop": true,
			"bones": {
				"head": {
					"rotation": ["Math.clamp(query.target_x_rotation, -15, 15)", "Math.clamp(query.target_y_rotation, -7.5, 7.5)", 0]
				}
			}
		},
		"animation.dodo.walk": {
			"loop": true,
			"anim_time_update": "query.modified_distance_moved",
			"bones": {
				"leftLeg": {
					"rotation": ["math.sin(query.anim_time * 30) * 30", 0, 0]
				},
				"rightLeg": {
					"rotation": ["math.sin(query.anim_time * 30) * -30", 0, 0]
				}
			}
		},
		"animation.dodo.fall": {
			"loop": true,
			"bones": {
				"leftLeg": {
					"rotation": ["math.sin(query.anim_time * 225) * 30", 0, 0]
				},
				"rightLeg": {
					"rotation": ["math.sin(query.anim_time * 225) * -30", 0, 0]
				},
				"leftWing": {
					"rotation": [0, 0, "math.abs(math.sin(query.anim_time * 225)*90)"]
				},
				"rightWing": {
					"rotation": [0, 0, "math.abs(math.sin(query.anim_time * 225)*90)*-1"]
				}
			}
		},
		"animation.dodo.sit": {
			"loop": true,
			"bones": {
				"body": {
					"position": [0, -4, 0]
				},
				"leftLeg": {
					"position": [0, 5, 0]
				},
				"rightLeg": {
					"position": [0, 5, 0]
				}
			}
		},
		"animation.dodo.christmas": {
			"loop": true,
			"bones": {
				"leftLeg": {
					"position": [0, -0.5, 0],
					"scale": 0.9
				},
				"rightLeg": {
					"position": [0, -0.5, 0],
					"scale": 0.9
				}
			}
		}
	}
}