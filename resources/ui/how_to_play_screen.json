                        {
                            "important_category@how_to_play_common.topic_category": {
                                "$category": "important"
                            }
                        },
                        {
                            "info_button@how_to_play.info_button": {
                                "$toggle_group_forced_index": 1
                            }
                        },
                        {
                            "license_button@how_to_play.license_button": {
                                "$toggle_group_forced_index": 2
                            }
                        },
                        {
                            "credits_button@how_to_play.credits_button": {
                                "$toggle_group_forced_index": 3
                            }
                        },
                        {
                            "about_me_button@how_to_play.about_me_button": {
                                "$toggle_group_forced_index": 4
                            }
                        },
                        {
