    "selector_stack_panel": {
        "type": "stack_panel",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "orientation": "vertical",
        "$default_selector_toggle_index": 1,
        "controls": [
            {
                "how_to_play_selector_pane": {
                    "type": "stack_panel",
                    "controls": [
                        {
                            "padding_0": {
                                "type": "panel",
                                "size": [0, 6]
                            }
                        },
                        {
                            "info_button@how_to_play.info_button": {
                                "$toggle_group_forced_index": 1
                            }
                        },
                        {
                            "license_button@how_to_play.license_button": {
                                "$toggle_group_forced_index": 2
                            }
                        },
                        {
                            "credits_button@how_to_play.credits_button": {
                                "$toggle_group_forced_index": 3
                            }
                        },
                        {
                            "about_me_button@how_to_play.about_me_button": {
                                "$toggle_group_forced_index": 4
                            }
                        },
                        {
                            "items_category@how_to_play_common.topic_category": {
                                "$category": "items"
                            }
                        },
                        {
                            "tools_button@how_to_play.tools_button": {
                                "$toggle_group_forced_index": 5
                            }
                        },						
                        {
                            "treats_button@how_to_play.treats_button": {
                                "$toggle_group_forced_index": 6
                            }
                        },
                        {
                            "mob_drops_button@how_to_play.mob_drops_button": {
                                "$toggle_group_forced_index": 7
                            }
                        },
                        {
                            "other_recipes_button@how_to_play.other_recipes_button": {
                                "$toggle_group_forced_index": 8
                            }
                        },						
                        {
                            "gameplay_category@how_to_play_common.topic_category": {
                                "$category": "gameplay"
                            }
                        },						
                        {
                            "taming_button@how_to_play.taming_button": {
                                "$toggle_group_forced_index": 9
                            }
                        },
                        {
                            "eggs_button@how_to_play.eggs_button": {
                                "$toggle_group_forced_index": 10
                            }
                        }
                    ]
                }
            }
        ]
    },
    "section_content_panels": {
        "type": "stack_panel",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "size": [
            "100% - 5px",
            "100%c + 5px"
        ],
        "offset": [2, 0],
        "controls": [
            {
                "general_tips_sections": {
                    "type": "stack_panel",
                    "controls": [
                        {
                            "section_contents_header@how_to_play_common.section_contents_header": {}
                        },
                        {
                            "info_section@how_to_play.info_section": {}
                        },
                        {
                            "license_section@how_to_play.license_section": {}
                        },
                        {
                            "credits_section@how_to_play.credits_section": {}
                        },
                        {
                            "about_me_section@how_to_play.about_me_section": {}
                        },
                        {
                            "tools_section@how_to_play.tools_section": {}
                        },						
                        {
                            "treats_section@how_to_play.treats_section": {}
                        },
                        {
                            "mob_drops_section@how_to_play.mob_drops_section": {}
                        },
                        {
                            "other_recipes_section@how_to_play.other_recipes_section": {}
                        },						
                        {
                            "taming_section@how_to_play.taming_section": {}
                        },
                        {
                            "eggs_section@how_to_play.eggs_section": {}
                        }
                    ]
                }
            }
        ]
    },
    "info_button@how_to_play_common.section_toggle_button": {
        "$section_topic": "info"
    },
    "info_section@how_to_play_common.main_sections": {
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "info_button_toggle",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "paragraph_1@how_to_play_common.paragraph": {
                    "$text": "Welp, hello there. The Vanilla Minecraft help section has now been replaced with this guide because, well, you're probably more interested in what this addon does rather then how to play Minecraft at this point."
                }
            }
        ]
    },
    
