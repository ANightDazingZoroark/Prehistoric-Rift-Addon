                "image_2@how_to_play_common.image": {
                    "texture": "textures\/ui\/rooted treat crafting",
                    "size": [
                        "100%",
                        "100px"
                    ]
                }
            },
            {
                "paragraph_4@how_to_play_common.paragraph": {
                    "$text": "A mix of root vegetables that serve as a starchy treat for most herbivores."
                }
            },
            {
                "padding_1@how_to_play_common.padding": {}
            },
            {
                "paragraph_5@how_to_play_common.paragraph": {
                    "$text": "Besides potatoes, you can also use carrots or beetroot."
                }
            },			
            {
                "header_3@how_to_play_common.header": {
                    "$text": "Leafy Treats"
                }
            },		
            {
                "image_3@how_to_play_common.image": {
                    "texture": "textures\/ui\/leafy treats crafting",
                    "size": [
                        "100%",
                        "100px"
                    ]
                }
            },
            {
                "paragraph_6@how_to_play_common.paragraph": {
                    "$text": "A mix of various types of leaves that serve as a tasty treat for herbivores. Especially leaf eating herbivores!"
                }
            },
            {
                "padding_2@how_to_play_common.padding": {}
            },
            {
                "paragraph_7@how_to_play_common.paragraph": {
                    "$text": "You can use any kind of leaf block to craft these treats."
                }
            },			
            {
                "header_4@how_to_play_common.header": {
                    "$text": "Meat Treat"
                }
            },		
            {
                "image_4@how_to_play_common.image": {
                    "texture": "textures\/ui\/flesh treat crafting",
                    "size": [
                        "100%",
                        "100px"
                    ]
                }
            },
            {
                "paragraph_8@how_to_play_common.paragraph": {
                    "$text": "Juicy, tender meat that has been wrapped around a bone as a tasty treat for most carnivores."
                }
            },
            {
                "padding_3@how_to_play_common.padding": {}
            },
            {
                "paragraph_9@how_to_play_common.paragraph": {
                    "$text": "All meat (except chicken and fish meats) can be used to craft this treat."
                }
            },			
            {
                "header_5@how_to_play_common.header": {
                    "$text": "Fish Treat"
                }
            },		
            {
                "image_5@how_to_play_common.image": {
                    "texture": "textures\/ui\/fish treat crafting",
                    "size": [
                        "100%",
                        "100px"
                    ]
                }
            },
            {
                "paragraph_10@how_to_play_common.paragraph": {
                    "$text": "A fish that has been punctured through the mouth with a bone. This serves as a tasty treat for fish eating creatures."
                }
            },
            {
                "padding_4@how_to_play_common.padding": {}
            },
            {
                "paragraph_11@how_to_play_common.paragraph": {
                    "$text": "Salmon can also be used to craft this treat."
                }
            }			
        ]
    }, 
 
