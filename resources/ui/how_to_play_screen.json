                        {
                            "mounted_weapons_button@how_to_play.mounted_weapons_button": {
                                "$toggle_group_forced_index": 13
                            }
                        }
                    ]
                }
            }
        ]
    },
    "section_content_panels": {
        "type": "stack_panel",
        "anchor_from": "top_left",
        "anchor_to": "top_left",
        "size": [
            "100% - 5px",
            "100%c + 5px"
