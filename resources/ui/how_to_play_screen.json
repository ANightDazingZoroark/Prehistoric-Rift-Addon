    "info_button@how_to_play_common.section_toggle_button": {
        "$section_topic": "info"
    },
    "info_section@how_to_play_common.main_sections": {
        "bindings": [
            {
                "binding_type": "view",
                "source_control_name": "info_button_toggle",
                "source_property_name": "#toggle_state",
                "target_property_name": "#visible"
            }
        ],
        "controls": [
            {
                "paragraph_1@how_to_play_common.paragraph": {
                    "$text": "Welp, hello there. The Vanilla Minecraft help section has now been replaced with this guide because, well, you're probably more interested in what this addon does rather then how to play Minecraft at this point."
                }
            }
        ]
    },
    
