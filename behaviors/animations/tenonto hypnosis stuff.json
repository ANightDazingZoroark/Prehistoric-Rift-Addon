{
	"format_version": "1.10.0",
	"animations": {
		"animation.find_tenontosaurus": {
			"animation_length": 0.05,
			"loop": true,
			"timeline": {
				"0": [
					"v.tenonto = 0.0; for_each(t.tenontosaurus, query.get_nearby_entities_except_self(12, 'rift:tenontosaurus'), { v.tenonto = v.tenonto + !(t.tenontosaurus -> query.is_tamed); }); return v.tenonto > 0.0;"
				]
			}
        },
		"animation.hypnosis_death_timer": {
			"animation_length": 11,
			"loop": false,
			"timeline": {
				"10": [
					"/kill @s"
				]
			}
        },
		"animation.hypnosis_death_timer_tamed": {
			"animation_length": 46,
			"loop": false,
			"timeline": {
				"45": [
					"/kill @s"
				]
			}
        },
		"animation.find_hypnotized": {
			"animation_length": 0.05,
			"loop": true,
			"timeline": {
				"0": [
					"v.hypnotized = 0.0; for_each(t.hypno, query.get_nearby_entities_except_self(16), { v.hypnotized = v.hypnotized + (!(t.hypno -> query.is_tamed) && (t.hypno -> query.property('rift:is_hypnotized'))); }); return v.hypnotized > 0.0;"
				]
			}
        },
        "animation.use_hypnosis_wild": {
