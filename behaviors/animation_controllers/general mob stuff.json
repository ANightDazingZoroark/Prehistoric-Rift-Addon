	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.add_moving_tag": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"hasTag": "query.is_moving"
						}
					]
				},
				"hasTag": {
					"transitions": [
						{
							"default": "!query.is_moving"
						}
					],
                    "on_entry": [
                        "/tag @s add moving"
                    ],
                    "on_exit": [
                        "/tag @s remove moving"
                    ]
				}
			}
		},
