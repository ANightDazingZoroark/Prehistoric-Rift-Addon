{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.kill_harvest_entity": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"die": "query.is_sheared"
						}
					],
					"on_exit": [
						"/kill @s"
					]
				},
				"die": {
					"on_entry": [
						"/kill @s"
					]
				}
			}
		}
	}
}