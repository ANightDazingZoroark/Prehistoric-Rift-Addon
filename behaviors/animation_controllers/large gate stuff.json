                "default": {
                    "transitions": [
                        {
                            "hitboxOneDir1": "query.body_y_rotation >= -45 && query.body_y_rotation <= 45"
                        },
                        {
                            "hitboxTwoDir1": "query.body_y_rotation > 45 && query.body_y_rotation < 135"
                        },
                        {
                            "hitboxThreeDir1": "(query.body_y_rotation >= 135 && query.body_y_rotation <= 180) || (query.body_y_rotation <= -135 && query.body_y_rotation >= -180)"
                        },
                        {
                            "hitboxFourDir1": "query.body_y_rotation < -45 && query.body_y_rotation > -135"
                        }
                    ]
                },

