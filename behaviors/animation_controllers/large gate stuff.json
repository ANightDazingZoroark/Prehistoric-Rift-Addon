                    "transitions": [
                        {
                            "hitboxOneDir2": "query.mark_variant == 1"
                        },
                        {
                            "hitboxOneDir3": "query.mark_variant == 2"
                        },
                        {
                            "hitboxOneDir4": "query.mark_variant == 3"
                        }
