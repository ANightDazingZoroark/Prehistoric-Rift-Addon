        "controller.animation.gate_add_phantom_tag": {
            "initial_state": "default",
            "states": {
                "default": {
                    "transitions": [
                        {
                            "isPhantom": "query.variant > 0"
                        }
                    ]
                },
                "isPhantom": {
                    "transitions": [
                        {
                            "default": "query.variant == 0"
                        }
                    ],
                    "on_entry": [
                        "/tag @s add isPhantom"
                    ],
