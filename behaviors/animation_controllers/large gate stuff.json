{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.gate_set_rotation": {
            "initial_state": "default",
            "states": {
                "default": {
                    "transitions": [
                        {
                            "setRotation": "(1.0)"
                        }
                    ]
                },
                "setRotation": {
                    "transitions": [
                        {
                            "default": "(0.0)"
                        }
                    ],
                    "on_entry": [
                        "/tp @s ~ ~ ~ 0"
                    ]
                }
            }
        }
    }
}