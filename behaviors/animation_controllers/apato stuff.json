{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.apato_forced_use_stomp": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"stomping": "query.is_sheared"
						}
					]
				},
				"stomping": {
					"transitions": [
						{
							"default": "!query.is_sheared"
						}
					],
					"animations": [
						"animForcedAttack"
					]
				}
			}
		}
	}
}