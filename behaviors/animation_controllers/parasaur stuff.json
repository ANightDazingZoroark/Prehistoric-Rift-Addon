{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.parasaur_forced_use_attack": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"stabbing": "query.is_powered"
						}
					]
				},
				"stabbing": {
					"transitions": [
						{
							"default": "!query.is_powered"
						}
					],
					"animations": [
						"animForcedAttack"
					]
				}
			}
		},
		"controller.animation.parasaur_eat_dropped_wheat": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"animFindWheat"
					],
					"transitions": [
						{
							"eating": "v.wheatItem > 0"
						}
					]
				},
				"eating": {
					"transitions": [
						{
							"default": "v.wheatItem == 0"
						}
					],
					"animations": [
						"animForcedAttack"
					]
				}
			}
		}
	}
}