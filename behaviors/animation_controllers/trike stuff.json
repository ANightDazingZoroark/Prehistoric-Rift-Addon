{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.trike_forced_use_stab": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"stabbing": "query.is_ignited"
						}
					]
				},
				"stabbing": {
					"transitions": [
						{
							"default": "!query.is_ignited"
						}
