{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.triceratops_wild_charge": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"charging": "query.is_angry && !query.is_baby"
						}
					]
				},
				"charging": {
					"transitions": [
						{
							"default": "!query.is_angry && !query.is_baby"
						}
					],
					"on_entry": [
						"/effect @s speed 60 3"
					],
					"on_exit": [
						"/effect @s clear"
					]
				}
			}
		}
	}
}