{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "description": {
            "identifier": "rift:egg",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            //tyranno egg
            "rift:tyrannosaurus_egg": {
                "minecraft:variant": {
                    "value": 0
                },
                "minecraft:interact": {
                    "interactions": [
                        {
                            "on_interact": {
                                "filters": {
                                    "test": "is_sneaking",
                                    "subject": "other",
                                    "value": true
                                },
                                "event": "rift:tyranno_egg_commit_disappear",
                                "target": "self"
                            },
                            "spawn_items": {
                                "table": "loot_tables/eggs/tyrannosaurus egg.loot.json"
                            }
                        }
                    ]
                }
            },
            "rift:tyrannosaurus_egg_not_hatching": {
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "filters": {
                                "any_of": [
                                    {
                                        "test": "is_biome",
                                        "value": "desert"
                                    },
                                    {
                                        "test": "is_biome",
                                        "value": "mesa"
                                    },
                                    {
                                        "test": "in_nether"
                                    }
                                ]
                            },
                            "event": "rift:tyrannosaurus_egg_start_hatching",
                            "target": "self"
                        }
                    ]
                }
            },
            "rift:tyrannosaurus_egg_hatching": {
                "minecraft:is_sheared": {},
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_biome",
                                        "operator": "!=",
                                        "value": "desert"
                                    },
                                    {
                                        "test": "is_biome",
                                        "operator": "!=",
                                        "value": "mesa"
                                    },
                                    {
                                        "test": "in_nether",
                                        "value": false
                                    }
                                ]
                            },
                            "event": "rift:tyrannosaurus_egg_stop_hatching",
                            "target": "self"
                        }
                    ]
                },
                "minecraft:timer": {
                    "time": 600,
                    "time_down_event": {
                        "event": "rift:tyrannosaurus_egg_hatched",
                        "target": "self"
                    }
                }
            },
            "rift:tyrannosaurus_egg_hatched": {
                "minecraft:transformation": {
                    "into": "rift:tyrannosaurus<rift:spawn_as_baby>"
                }
            },
            //stego egg
            "rift:stegosaurus_egg": {
                "minecraft:variant": {
                    "value": 1
                },
                "minecraft:interact": {
                    "interactions": [
                        {
                            "on_interact": {
                                "filters": {
                                    "test": "is_sneaking",
                                    "subject": "other",
                                    "value": true
                                },
                                "event": "rift:stego_egg_commit_disappear",
                                "target": "self"
                            },
                            "spawn_items": {
                                "table": "loot_tables/eggs/stegosaurus egg.loot.json"
                            }
                        }
                    ]
                }
            },
            "rift:stegosaurus_egg_not_hatching": {
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "filters": {
                                "any_of": [
                                    {
                                        "test": "is_biome",
                                        "value": "desert"
                                    },
                                    {
                                        "test": "is_biome",
                                        "value": "mesa"
                                    },
                                    {
                                        "test": "in_nether"
                                    }
                                ]
                            },
                            "event": "rift:stegosaurus_egg_start_hatching",
                            "target": "self"
                        }
                    ]
                }
            },
            "rift:stegosaurus_egg_hatching": {
                "minecraft:is_sheared": {},
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_biome",
                                        "operator": "!=",
                                        "value": "desert"
                                    },
                                    {
                                        "test": "is_biome",
                                        "operator": "!=",
                                        "value": "mesa"
                                    },
                                    {
                                        "test": "in_nether",
                                        "value": false
                                    }
                                ]
                            },
                            "event": "rift:stegosaurus_egg_stop_hatching",
                            "target": "self"
                        }
                    ]
                },
                "minecraft:timer": {
                    "time": 300,
                    "time_down_event": {
                        "event": "rift:stegosaurus_egg_hatched",
                        "target": "self"
                    }
                }
            },
            "rift:stegosaurus_egg_hatched": {
                "minecraft:transformation": {
                    "into": "rift:stegosaurus<rift:spawn_as_baby>"
                }
            }
        },
        "components": {
            "minecraft:health": {
                "value": 1
            },
            "minecraft:physics": {
                "has_collision": true,
                "has_gravity": true
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "rift:tyrannosaurus_egg",
                        "rift:tyrannosaurus_egg_not_hatching"
                    ]
                }
            },
            //tyranno egg
            "rift:summon_tyrannosaurus_egg": {
                "add": {
                    "component_groups": [
                        "rift:tyrannosaurus_egg",
                        "rift:tyrannosaurus_egg_not_hatching"
                    ]
                }
            },
            "rift:tyrannosaurus_egg_start_hatching": {
                "add": {
                    "component_groups": [
                        "rift:tyrannosaurus_egg_hatching"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "rift:tyrannosaurus_egg_not_hatching"
                    ]
                },
                "run_command": {
                    "command": [
                        "say start hatching"
                    ]
                }
            },
            "rift:tyrannosaurus_egg_stop_hatching": {
                "add": {
                    "component_groups": [
                        "rift:tyrannosaurus_egg_not_hatching"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "rift:tyrannosaurus_egg_hatching"
                    ]
                }
            },
            "rift:tyrannosaurus_egg_hatched": {
                "add": {
                    "component_groups": [
                        "rift:tyrannosaurus_egg_hatched"
                    ]
                }
            },
            "rift:tyranno_egg_commit_disappear": {
                "run_command": {
                    "command": [
                        "tp @s ~ ~-40 ~"
                    ]
                }
            },
            //stego egg
            "rift:summon_stegosaurus_egg": {
                "add": {
                    "component_groups": [
                        "rift:stegosaurus_egg",
                        "rift:stegosaurus_egg_not_hatching"
                    ]
                }
            },
            "rift:stegosaurus_egg_start_hatching": {
                "add": {
                    "component_groups": [
                        "rift:stegosaurus_egg_hatching"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "rift:stegosaurus_egg_not_hatching"
                    ]
                },
                "run_command": {
                    "command": [
                        "say start hatching"
                    ]
                }
            },
            "rift:stegosaurus_egg_stop_hatching": {
                "add": {
                    "component_groups": [
                        "rift:stegosaurus_egg_not_hatching"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "rift:stegosaurus_egg_hatching"
                    ]
                }
            },
            "rift:stegosaurus_egg_hatched": {
                "add": {
                    "component_groups": [
                        "rift:stegosaurus_egg_hatched"
                    ]
                }
            },
            "rift:stego_egg_commit_disappear": {
                "run_command": {
                    "command": [
                        "tp @s ~ ~-40 ~"
                    ]
                }
            }
        }
    }
}