				"triggers": [
					{
						"filters": {
							"all_of": [
								{
									"test": "has_mob_effect",
									"subject": "self",
									"value": "bad_omen"
								},
								{
									"test": "is_in_village",
									"subject": "self",
									"value": true
								}
							]
						},
						"event": "minecraft:trigger_raid"
					},
					{
						"filters": {
							"test": "has_tag",
							"subject": "self",
							"value": "climbMount"
						},
						"event": "rift:riding_climber",
						"target": "self"
					},
					{
						"filters": {
							"test": "has_tag",
							"subject": "self",
							"value": "nClimbMount"
						},
						"event": "rift:not_riding_climber",
						"target": "self"
					}
				]
			},
			"minecraft:damage_sensor": {
				"triggers": [
					{
						"on_damage": {
							"filters": {
								"all_of": [
									{
										"test": "is_family",
										"subject": "other",
										"value": "tyrannosaurus"
									},
									{
										"test": "has_component",
										"subject": "other",
										"operator": "!=",
										"value": "minecraft:baby"
									}
								]
							}
						},
						"deals_damage": true,
						"damage_multiplier": 2
					},
					{
						"on_damage": {
							"filters": {
								"test": "has_component",
								"subject": "self",
								"value": "minecraft:is_charged"
							}
						},
						"damage_multiplier": 0.25,
						"cause": "entity_attack"
					},
					{
						"on_damage": {
							"filters": {
								"test": "is_family",
								"subject": "other",
								"value": "sarcoSpinAttack"
							},
							"event": "rift:hit_by_sarco_spin",
							"target": "self"
						}
					}
				]
			}			
