{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "description": {
            "identifier": "rift:dilophosaurus_trap",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false
        },
        "component_groups": {
            "rift:untriggered": {
                "minecraft:is_charged": {},
                "minecraft:entity_sensor": {
                    "sensor_range": 3,
                    "minimum_count": 1,
                    "event_filters": {
                        "all_of": [
                            {
                                "test": "is_family",
                                "subject": "other",
                                "operator": "!=",
                                "value": "inanimate"
                            },
                            {
                                "test": "is_family",
                                "subject": "other",
                                "operator": "!=",
                                "value": "dilophosaurus"
                            },
                            {
                                "test": "is_riding",
                                "subject": "other",
                                "value": false
                            }
                        ]
                    },
                    "event": "rift:triggered"
                }
            },
            "rift:triggered": {
                "minecraft:spawn_entity": {
					"entities": [
						{
							"spawn_entity": "rift:dilophosaurus_trap",
							"single_use": true,
							"min_wait_time": 0,
							"max_wait_time": 0,
							"spawn_event": "rift:get_targets"
						}
					]
				},
                "minecraft:timer": {
                    "time": 0,
                    "time_down_event": {
                        "event": "rift:explode",
                        "target": "self"
                    }
                }
            },
            "rift:explode": {
                "minecraft:explode": {
                    "breaks_blocks": false,
                    "fuse_length": 0,
                    "fuse_lit": true,
                    "power": 3
                }
            },
            "rift:get_targets": {
                "minecraft:behavior.nearest_attackable_target": {
                    "priority": 1,
                    "within_radius": 32,
                    "entity_types": [
                        {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "!=",
                                        "value": "inanimate"
                                    },
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "!=",
                                        "value": "dilophosaurus"
                                    },
                                    {
                                        "test": "is_riding",
                                        "subject": "other",
                                        "value": false
                                    }
                                ]
                            },
                            "max_dist": 32
                        }
                    ]
                }
            },
            "rift:has_no_target": {
                "minecraft:on_target_acquired": {
                    "event": "rift:become_angry",
                    "target": "self"
                }
            },
            "rift:has_target": {
				"minecraft:angry": {
					"broadcast_anger": true,
					"duration": 5,
					"broadcast_range": 32,
					"broadcast_filters": {
						"test": "is_family",
						"subject": "other",
						"operator": "!=",
						"value": "minecraft:is_tamed"
					},
					"broadcast_targets": [
						"dilophosaurus"
					],
					"calm_event": {
						"event": "rift:commit_despawn",
						"target": "self"
					}
				}
            },
            "rift:despawn": {
                "minecraft:instant_despawn": {}
            }
        },
        "components": {
            "minecraft:type_family": {
				"family": [
					"dilophosaurus",
					"inanimate"
				]
            },
            "minecraft:collision_box": {
                "width": 0.01,
                "height": 0.01
            },
            "minecraft:custom_hit_test": {
                "hitboxes": [
                    {
                        "pivot": [
                            0, 
                            0,
                            0
                        ],
                        "width": 1,
                        "height": 1
                    }
                ]
            },
            "minecraft:is_sheared": {},
            "minecraft:physics": {},
            "minecraft:damage_sensor": {
                "triggers": {
                    "cause": "all",
                    "deals_damage": false
                }
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "rift:untriggered"
                    ]
                }
            },
            "rift:triggered": {
                "add": {
                    "component_groups": [
                        "rift:triggered"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "rift:untriggered"
                    ]
                }
            },
            "rift:explode": {
                "add": {
                    "component_groups": [
                        "rift:explode"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "rift:triggered"
                    ]
                }
            },
            "rift:get_targets": {
                "add": {
                    "component_groups": [
                        "rift:get_targets",
                        "rift:has_no_target"
                    ]
                }
            },
            "rift:become_angry": {
                "add": {
                    "component_groups": [
                        "rift:has_target"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "rift:has_no_target"
                    ]
                }
            },
            "rift:commit_despawn": {
                "add": {
                    "component_groups": [
                        "rift:despawn"
                    ]
                }
            }
        }
    }
}