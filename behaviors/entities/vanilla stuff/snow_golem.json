{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "component_groups": {
            "minecraft:snowman_sheared": {
                "minecraft:is_sheared": {}
            }
        },
        "components": {
            "minecraft:attack": {
                "damage": 2
            },
            "minecraft:balloonable": {},
            "minecraft:behavior.look_at_player": {
                "look_distance": 6.0,
                "priority": 3
            },
            "minecraft:behavior.nearest_attackable_target": {
                "entity_types": [
                    {
                        "filters": {
                            "subject": "other",
                            "test": "is_family",
                            "value": "monster"
                        },
                        "within_default": 6
                    },
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "carnivore"
                                },
                                {
                                    "operator": "!=",
                                    "subject": "other",
                                    "test": "has_component",
                                    "value": "minecraft:is_tamed"
                                }
                            ]
                        },
                        "within_default": 6
                    }
                ],
                "priority": 1
            },
            "minecraft:behavior.random_look_around": {
                "priority": 4
            },
            "minecraft:behavior.random_stroll": {
                "priority": 2,
                "speed_multiplier": 1
            },
            "minecraft:behavior.ranged_attack": {
                "attack_interval": 1,
                "attack_radius": 10,
                "priority": 1,
                "speed_multiplier": 1.25
            },
            "minecraft:breathable": {
                "suffocate_time": 0,
                "total_supply": 15
            },
            "minecraft:can_climb": {},
            "minecraft:collision_box": {
                "height": 1.8,
                "width": 0.4
            },
            "minecraft:conditional_bandwidth_optimization": {},
