            "minecraft:raid_expired": {
                "sequence": [
                    {
                        "filters": {
                            "test": "has_nametag",
                            "value": false
                        },
                        "remove": {
                            "component_groups": [
                                "minecraft:raid_persistence"
                            ]
                        }
                    }
                ]
            },
            "minecraft:spawn_for_raid": {
                "add": {
                    "component_groups": [
                        "minecraft:raid_configuration",
                        "minecraft:raid_despawn",
                        "minecraft:raid_persistence"
                    ]
                }
            },
            "minecraft:start_celebrating": {
                "sequence": [
                    {
                        "add": {
                            "component_groups": [
                                "minecraft:celebrate"
                            ]
                        }
                    },
                    {
                        "filters": {
                            "test": "has_nametag",
                            "value": false
                        },
                        "remove": {
                            "component_groups": [
                                "minecraft:raid_persistence"
                            ]
                        }
                    }
                ]
            },
            "minecraft:stop_celebrating": {
                "remove": {
                    "component_groups": [
                        "minecraft:celebrate"
                    ]
                }
            },
            "rift:start_bleeding": {
                "set_property": {
                    "rift:bleeding": true
                }
            },
            "rift:stop_bleeding": {
                "set_property": {
                    "rift:bleeding": false
                }
            }
