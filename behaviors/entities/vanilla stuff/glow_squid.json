{
    "format_version": "1.18.10",
    "minecraft:entity": {
        "component_groups": {
            "minecraft:squid_baby": {
                "minecraft:is_baby": {},
                "minecraft:scale": {
                    "value": 0.5
                }
            }
        },
        "components": {
            "minecraft:attack": {
                "damage": 0
            },
            "minecraft:balloonable": {
                "mass": 0.5
            },
            "minecraft:behavior.avoid_mob_type": {
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "parasaurScary"
                                },
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "carnivore"
                                }
                            ]
                        },
                        "max_dist": 12,
                        "must_see": true,
                        "sprint_speed_multiplier": 1.2,
                        "walk_speed_multiplier": 1.2
                    }
                ],
                "priority": 0
            },
            "minecraft:behavior.melee_attack": {
                "priority": 2
            },
            "minecraft:behavior.nearest_attackable_target": {
                "entity_types": [
                    {
                        "filters": {
                            "subject": "other",
                            "test": "is_family",
                            "value": "parasaurAngerer"
                        },
                        "max_dist": 64
                    }
                ],
                "priority": 1
            },
            "minecraft:behavior.squid_dive": {
                "priority": 4
            },
            "minecraft:behavior.squid_flee": {
                "priority": 4
            },
            "minecraft:behavior.squid_idle": {
                "priority": 4
            },
            "minecraft:behavior.squid_move_away_from_ground": {
                "priority": 3
            },
            "minecraft:behavior.squid_out_of_water": {
                "priority": 4
            },
            "minecraft:breathable": {
                "breathes_air": false,
                "breathes_water": true,
                "suffocate_time": 0,
                "total_supply": 15
            },
            "minecraft:can_climb": {},
            "minecraft:collision_box": {
                "height": 0.95,
                "width": 0.95
            },
            "minecraft:despawn": {
                "despawn_from_distance": {}
            },
            "minecraft:experience_reward": {
                "on_death": "!query.is_baby && query.last_hit_by_player ? Math.Random(1,3) : 0"
            },
            "minecraft:health": {
                "max": 10,
                "value": 10
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "cause": "lava",
                        "damage_per_tick": 4,
                        "filters": {
                            "operator": "==",
                            "subject": "self",
                            "test": "in_lava",
                            "value": true
                        }
                    }
                ]
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:jump.static": {},
            "minecraft:leashable": {
                "hard_distance": 6.0,
                "max_distance": 10.0,
                "soft_distance": 4.0
            },
            "minecraft:loot": {
                "table": "loot_tables/entities/glow_squid.json"
            },
            "minecraft:movement": {
                "value": 0.2
            },
            "minecraft:movement.basic": {},
            "minecraft:nameable": {},
            "minecraft:navigation.walk": {
                "can_path_over_water": true,
                "can_sink": false
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:type_family": {
                "family": [
                    "squid",
                    "mob",
                    "animal"
                ]
            }
        },
        "description": {
            "animations": {
                "animBleed": "animation.general_bleed",
                "animBleedCountdown": "animation.general_bleed_countdown",
                "contBleed": "controller.animation.general_manage_bleed",
                "contMovingTag": "controller.animation.add_moving_tag"
            },
            "identifier": "minecraft:glow_squid",
            "is_experimental": false,
            "is_spawnable": true,
            "is_summonable": true,
            "properties": {
                "rift:bleeding": {
                    "client_sync": true,
                    "default": false,
                    "type": "bool"
                }
            },
            "scripts": {
                "animate": [
                    "contBleed",
                    "contMovingTag"
                ]
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "randomize": [
                    {
                        "add": {},
                        "remove": {},
                        "weight": 95
                    },
                    {
                        "add": {
                            "component_groups": [
                                "minecraft:squid_baby"
                            ]
                        },
                        "remove": {},
                        "weight": 5
                    }
                ]
            },
            "rift:start_bleeding": {
                "set_property": {
                    "rift:bleeding": true
                }
            },
            "rift:stop_bleeding": {
                "set_property": {
                    "rift:bleeding": false
                }
            }
        }
    }
}