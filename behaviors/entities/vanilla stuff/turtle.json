{
    "format_version": "1.18.20",
    "minecraft:entity": {
        "component_groups": {
            "minecraft:adult": {
                "minecraft:behavior.breed": {
                    "priority": 5,
                    "speed_multiplier": 1.0
                },
                "minecraft:behavior.move_to_land": {
                    "goal_radius": 0.5,
                    "priority": 8,
                    "search_height": 5,
                    "search_range": 16
                },
                "minecraft:behavior.random_stroll": {
                    "interval": 100,
                    "priority": 10
                },
                "minecraft:breedable": {
                    "breed_items": [
                        "seagrass"
                    ],
                    "breeds_with": {
                        "baby_type": "minecraft:turtle",
                        "breed_event": {
                            "event": "minecraft:become_pregnant",
                            "target": "self"
                        },
                        "mate_type": "minecraft:turtle"
                    },
                    "causes_pregnancy": true,
                    "require_tame": false
                },
                "minecraft:collision_box": {
                    "height": 0.4,
                    "width": 1.2
                },
                "minecraft:experience_reward": {
                    "on_bred": "Math.Random(1,7)",
                    "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
                },
                "minecraft:loot": {
                    "table": "loot_tables/entities/sea_turtle.json"
                },
                "minecraft:type_family": {
                    "family": [
                        "turtle",
                        "mob",
                        "animal"
                    ]
                },
                "minecraft:underwater_movement": {
                    "value": 0.12
                }
            },
            "minecraft:baby": {
                "minecraft:ageable": {
                    "drop_items": [
                        "turtle_shell_piece"
                    ],
                    "duration": 1200,
                    "feed_items": [
                        "seagrass"
                    ],
                    "grow_up": {
                        "event": "minecraft:ageable_grow_up",
                        "target": "self"
                    }
                },
                "minecraft:behavior.move_to_water": {
                    "goal_radius": 0.1,
                    "priority": 1,
                    "search_height": 5,
                    "search_range": 15
                },
                "minecraft:collision_box": {
                    "height": 0.2,
                    "width": 0.6
                },
                "minecraft:is_baby": {},
                "minecraft:scale": {
                    "value": 0.16
                },
                "minecraft:type_family": {
                    "family": [
                        "turtle",
                        "baby_turtle",
                        "mob",
                        "animal"
                    ]
                },
                "minecraft:underwater_movement": {
                    "value": 0.06
                }
            },
            "minecraft:pregnant": {
                "minecraft:behavior.go_home": {
                    "goal_radius": 4.0,
                    "interval": 700,
                    "on_home": [
                        {
                            "event": "minecraft:go_lay_egg",
                            "target": "self"
                        }
                    ],
                    "priority": 1,
                    "speed_multiplier": 1.0
                }
            },
            "minecraft:wants_to_lay_egg": {
                "minecraft:behavior.lay_egg": {
                    "goal_radius": 1.5,
                    "on_lay": {
                        "event": "minecraft:laid_egg",
                        "target": "self"
                    },
                    "priority": 1,
                    "search_height": 4,
                    "search_range": 16,
                    "speed_multiplier": 1.0
                }
            }
        },
        "components": {
            "minecraft:attack": {
                "damage": 0
            },
            "minecraft:behavior.avoid_mob_type": {
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "parasaurScary"
                                },
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "carnivore"
                                }
                            ]
                        },
                        "max_dist": 12,
                        "must_see": true,
                        "sprint_speed_multiplier": 1.2,
                        "walk_speed_multiplier": 1.2
                    }
                ],
                "priority": 2
            },
            "minecraft:behavior.hurt_by_target": {
                "priority": 1
            },
            "minecraft:behavior.look_at_player": {
                "priority": 11,
                "probability": 0.02,
                "target_distance": 6.0
            },
            "minecraft:behavior.melee_attack": {
                "priority": 4
            },
            "minecraft:behavior.move_to_water": {
                "goal_radius": 1.5,
                "priority": 7,
                "search_height": 5,
                "search_range": 16
            },
            "minecraft:behavior.nearest_attackable_target": {
                "entity_types": [
                    {
                        "filters": {
                            "subject": "other",
                            "test": "is_family",
                            "value": "parasaurAngerer"
                        },
                        "max_dist": 64
                    }
                ],
                "priority": 3
            },
            "minecraft:behavior.panic": {
                "prefer_water": true,
                "priority": 0,
                "speed_multiplier": 1.2
            },
            "minecraft:behavior.random_swim": {
                "interval": 0,
                "priority": 9,
                "xz_dist": 30,
                "y_dist": 15
            },
            "minecraft:behavior.tempt": {
                "can_tempt_vertically": true,
                "items": [
                    "seagrass"
                ],
                "priority": 6,
                "speed_multiplier": 1.1
            },
            "minecraft:breathable": {
                "breathes_air": true,
                "breathes_water": true,
                "generates_bubbles": false,
                "suffocate_time": 0,
                "total_supply": 15
            },
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:damage_sensor": {
                "triggers": {
                    "cause": "lightning",
                    "damage_multiplier": 2000.0,
                    "deals_damage": true
                }
            },
            "minecraft:despawn": {
                "despawn_from_distance": {}
            },
            "minecraft:follow_range": {
                "value": 1024
            },
            "minecraft:health": {
                "value": 30
            },
            "minecraft:home": {},
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "cause": "lava",
                        "damage_per_tick": 4,
                        "filters": {
                            "operator": "==",
                            "subject": "self",
                            "test": "in_lava",
                            "value": true
                        }
                    }
                ]
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:jump.static": {},
            "minecraft:movement": {
                "value": 0.1
            },
            "minecraft:movement.amphibious": {
                "max_turn": 5.0
            },
            "minecraft:nameable": {},
            "minecraft:navigation.generic": {
                "avoid_damage_blocks": true,
                "can_path_over_water": false,
                "can_sink": false,
                "can_swim": true,
                "can_walk": true,
                "is_amphibious": true
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:water_movement": {
                "drag_factor": 0.9
            }
        },
        "description": {
            "animations": {
                "animBleed": "animation.general_bleed",
                "animBleedCountdown": "animation.general_bleed_countdown",
                "contBleed": "controller.animation.general_manage_bleed",
                "contMovingTag": "controller.animation.add_moving_tag"
            },
            "identifier": "minecraft:turtle",
            "is_experimental": false,
            "is_spawnable": true,
            "is_summonable": true,
            "properties": {
                "rift:bleeding": {
                    "client_sync": true,
                    "default": false,
                    "type": "bool"
                }
            },
            "scripts": {
                "animate": [
                    "contBleed",
                    "contMovingTag"
                ]
            }
        },
        "events": {
            "minecraft:ageable_grow_up": {
                "add": {
                    "component_groups": [
                        "minecraft:adult"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:baby"
                    ]
                }
            },
            "minecraft:become_pregnant": {
                "add": {
                    "component_groups": [
                        "minecraft:pregnant"
                    ]
                }
            },
            "minecraft:entity_born": {
                "add": {
                    "component_groups": [
                        "minecraft:baby"
                    ]
                }
            },
            "minecraft:entity_spawned": {
                "randomize": [
                    {
                        "add": {
                            "component_groups": [
                                "minecraft:adult"
                            ]
                        },
                        "weight": 9
                    },
                    {
                        "add": {
                            "component_groups": [
                                "minecraft:baby"
                            ]
                        },
                        "weight": 1
                    }
                ]
            },
            "minecraft:go_lay_egg": {
                "add": {
                    "component_groups": [
                        "minecraft:wants_to_lay_egg"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:pregnant"
                    ]
                }
            },
            "minecraft:laid_egg": {
                "remove": {
                    "component_groups": [
                        "minecraft:wants_to_lay_egg"
                    ]
                }
            },
            "rift:start_bleeding": {
                "set_property": {
                    "rift:bleeding": true
                }
            },
            "rift:stop_bleeding": {
                "set_property": {
                    "rift:bleeding": false
                }
            }
        }
    }
}