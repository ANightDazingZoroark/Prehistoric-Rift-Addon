{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "component_groups": {
            "minecraft:lightning_immune": {
                "minecraft:damage_sensor": {
                    "triggers": {
                        "deals_damage": false,
                        "on_damage": {
                            "filters": {
                                "subject": "other",
                                "test": "is_family",
                                "value": "lightning"
                            }
                        }
                    }
                }
            },
            "minecraft:skeleton_horse_adult": {
                "minecraft:collision_box": {
                    "height": 1.6,
                    "width": 1.4
