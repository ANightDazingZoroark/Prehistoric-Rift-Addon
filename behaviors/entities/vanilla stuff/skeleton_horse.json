                    {
                        "filters": {
                            "subject": "other",
                            "test": "is_family",
                            "value": "parasaurScary"
                        },
                        "max_dist": 12,
                        "must_see": true,
                        "sprint_speed_multiplier": 1.2,
                        "walk_speed_multiplier": 1.2
                    }
