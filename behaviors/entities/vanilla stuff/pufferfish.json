                        "none_of": [
                            {
                                "subject": "other",
                                "test": "is_family",
                                "value": "mob"
                            },
                            {
                                "all_of": [
                                    {
                                        "subject": "other",
                                        "test": "is_family",
                                        "value": "player"
                                    },
                                    {
                                        "none_of": {
                                            "subject": "other",
                                            "test": "has_ability",
                                            "value": "instabuild"
                                        }
                                    }
                                ]
                            }
                        ],
                        "operator": ">",
                        "test": "distance_to_nearest_player",
