      },
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:navigation.walk": {
        "is_amphibious": true,
        "can_path_over_water": false,
        "avoid_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:fire_immune": {
      },
      "minecraft:leashable": {
        "soft_distance": 4.0,
        "hard_distance": 6.0,
        "max_distance": 10.0
      },
      "minecraft:balloonable": {
      },
      "minecraft:despawn": {
        "filters": {
          "any_of": [
            {
              "all_of": [
                {
                  "test": "is_persistent",
                  "value": false
                },
                {
                  "test": "distance_to_nearest_player",
                  "operator": ">",
                  "value": 54
                }
              ]
            },
            {
              "all_of": [
                {
                  "test": "is_persistent",
                  "value": false
                },
                {
                  "test": "inactivity_timer",
                  "subject": "self",
                  "value": 30
                },
                {
                  "test": "random_chance",
                  "value": 800
                },
                {
                  "test": "distance_to_nearest_player",
                  "operator": ">",
                  "value": 32
                }
              ]
            }
          ]
        }
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {
      }
    },

    "events": {
      "minecraft:entity_transformed": {
        "sequence": [
          {
            "filters": {
              "test": "has_component",
              "subject": "other",
              "value": "minecraft:is_baby"
            },
            "add": {
              "component_groups": [
                "zoglin_baby",
                "rift:not_hypnotized"
              ]
            }
          },
          {
            "filters": {
              "test": "has_component",
              "subject": "other",
              "operator": "!=",
              "value": "minecraft:is_baby"
            },
            "add": {
              "component_groups": [
                "zoglin_adult",
                "rift:not_hypnotized"
              ]
            }
          }
        ]
      },
      "minecraft:entity_spawned": {
        "randomize": [
          {
            "weight": 95,
            "add": {
              "component_groups": [
                "zoglin_adult",
                "rift:not_hypnotized"
              ]
            }
          },
          {
            "weight": 5,
            "add": {
              "component_groups": [
                "zoglin_baby",
                "rift:not_hypnotized"
              ]
            }
          }
        ]
      },

      "minecraft:as_adult": {
        "add": {
          "component_groups": [
            "zoglin_adult",
            "rift:not_hypnotized"
          ]
        }
      },
      "minecraft:as_baby": {
        "add": {
          "component_groups": [
            "zoglin_baby",
            "rift:not_hypnotized"
          ]
        }
      },
      
      "rift:spawn_hypnotized_by_tenontosaurus": {
        "randomize": [
          {
            "weight": 95,
            "add": {
              "component_groups": [
                "zoglin_adult",
                "rift:follow_tenontosaurus",
                "rift:follow_tenontosaurus_no_target"
              ]
            },
            "set_property": {
                "rift:is_hypnotized": true
            }
          },
          {
            "weight": 5,
            "add": {
              "component_groups": [
                "zoglin_baby",
                "rift:follow_tenontosaurus",
                "rift:follow_tenontosaurus_no_target"
              ]
            },
            "set_property": {
                "rift:is_hypnotized": true
            }
          }
        ]
      },
      "rift:attack_for_tenontosaurus": {
          "add": {
              "component_groups": [
                  "rift:attacking_for_tenontosaurus"
              ]
          },
          "remove": {
              "component_groups": [
                  "rift:follow_tenontosaurus_no_target"
              ]
          },
          "run_command": {
              "command": [
                  "say FOR THE QUEEN"
              ]
          }
      },
      "rift:stop_attacking_for_tenontosaurus": {
          "add": {
              "component_groups": [
                  "rift:follow_tenontosaurus_no_target"
              ]
          },
          "remove": {
              "component_groups": [
                  "rift:attacking_for_tenontosaurus"
              ]
          }
      },
      "rift:hypnotized_by_tenontosaurus": {
        "add": {
            "component_groups": [
                "rift:transform_into_hypnotized"
            ]
        }
      },
      "rift:transform_into_hypnotized": {
        "sequence": [
          {
            "filters": {
              "test": "has_component",
              "subject": "other",
              "value": "minecraft:is_baby"
            },
            "add": {
              "component_groups": [
                "zoglin_baby",
                "rift:follow_tenontosaurus",
                "rift:follow_tenontosaurus_no_target"
              ]
            },
            "set_property": {
                "rift:is_hypnotized": true
            }
          },
          {
            "filters": {
              "test": "has_component",
              "subject": "other",
              "operator": "!=",
              "value": "minecraft:is_baby"
            },
            "add": {
              "component_groups": [
                "zoglin_adult",
                "rift:follow_tenontosaurus",
                "rift:follow_tenontosaurus_no_target"
              ]
            },
            "set_property": {
                "rift:is_hypnotized": true
            }
          }
        ]
