{
    "format_version": "1.13.0",
    "minecraft:entity": {
        "component_groups": {
            "dragon_death": {
                "minecraft:behavior.dragondeath": {
                    "priority": 0
                }
            },
            "dragon_flying": {
                "minecraft:behavior.dragonchargeplayer": {
                    "priority": 1
                },
                "minecraft:behavior.dragonholdingpattern": {
                    "priority": 3
                },
                "minecraft:behavior.dragonstrafeplayer": {
                    "priority": 2
                },
                "minecraft:behavior.dragontakeoff": {
                    "priority": 0
                },
                "minecraft:shooter": {
                    "def": "minecraft:dragon_fireball",
                    "type": "dragonfireball"
                }
            },
            "dragon_sitting": {
                "minecraft:behavior.dragonflaming": {
                    "priority": 1
                },
                "minecraft:behavior.dragonlanding": {
                    "priority": 0
                },
                "minecraft:behavior.dragonscanning": {
                    "priority": 2
                }
            }
        },
        "components": {
            "minecraft:attack": {
                "damage": 3
            },
            "minecraft:boss": {
                "hud_range": 125,
                "should_darken_sky": false
            },
            "minecraft:collision_box": {
                "height": 4,
                "width": 13
            },
            "minecraft:damage_sensor": {
                "triggers": {
                    "cause": "fall",
                    "deals_damage": false
                }
            },
            "minecraft:fire_immune": true,
            "minecraft:flying_speed": {
                "value": 0.6
            },
            "minecraft:health": {
                "max": 200,
                "value": 200
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:knockback_resistance": {
                "max": 100,
                "value": 100
            },
            "minecraft:movement": {
                "value": 0.3
            },
            "minecraft:on_death": {
                "event": "minecraft:start_death",
                "target": "self"
            },
            "minecraft:on_start_landing": {
                "event": "minecraft:start_land",
                "target": "self"
            },
            "minecraft:on_start_takeoff": {
                "event": "minecraft:start_fly",
                "target": "self"
            },
            "minecraft:persistent": {},
            "minecraft:physics": {
                "has_collision": false,
                "has_gravity": false
            },
            "minecraft:type_family": {
                "family": [
                    "dragon",
                    "mob",
                    "boss"
                ]
            }
        },
        "description": {
            "animations": {
                "animBleed": "animation.general_bleed",
                "animBleedCountdown": "animation.general_bleed_countdown",
                "contBleed": "controller.animation.general_manage_bleed"
            },
            "identifier": "minecraft:ender_dragon",
            "is_experimental": false,
            "is_spawnable": false,
            "is_summonable": true,
            "properties": {
                "rift:bleeding": {
                    "client_sync": true,
                    "default": false,
                    "type": "bool"
                }
            },
            "scripts": {
                "animate": [
                    "contBleed"
                ]
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "dragon_flying"
                    ]
                },
                "remove": {}
            },
            "minecraft:start_death": {
                "add": {
                    "component_groups": [
                        "dragon_death"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "dragon_sitting",
                        "dragon_flying"
                    ]
                }
            },
            "minecraft:start_fly": {
                "add": {
                    "component_groups": [
                        "dragon_flying"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "dragon_sitting"
                    ]
                }
            },
            "minecraft:start_land": {
                "add": {
                    "component_groups": [
                        "dragon_sitting"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "dragon_flying"
                    ]
                }
            },
            "rift:start_bleeding": {
                "set_property": {
                    "rift:bleeding": true
                }
            },
            "rift:stop_bleeding": {
                "set_property": {
                    "rift:bleeding": false
                }
            }
        }
    }
}