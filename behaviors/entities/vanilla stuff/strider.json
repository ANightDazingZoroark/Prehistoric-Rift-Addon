{
    "format_version": "1.18.10",
    "minecraft:entity": {
        "component_groups": {
            "minecraft:detect_suffocating": {
                "minecraft:environment_sensor": {
                    "triggers": {
                        "event": "start_suffocating",
                        "filters": {
                            "all_of": [
                                {
                                    "operator": "==",
                                    "subject": "self",
                                    "test": "in_lava",
                                    "value": false
                                },
                                {
                                    "any_of": [
                                        {
                                            "operator": "==",
                                            "subject": "self",
                                            "test": "is_riding",
                                            "value": false
                                        },
                                        {
                                            "operator": "==",
                                            "subject": "other",
                                            "test": "in_lava",
                                            "value": false
                                        }
                                    ]
                                }
                            ]
                        }
                    }
                }
            },
            "minecraft:start_suffocating": {
                "minecraft:environment_sensor": {
                    "triggers": {
                        "event": "stop_suffocating",
                        "filters": {
                            "any_of": [
                                {
                                    "operator": "==",
                                    "subject": "self",
                                    "test": "in_lava",
                                    "value": true
                                },
                                {
                                    "operator": "==",
                                    "subject": "other",
                                    "test": "in_lava",
                                    "value": true
                                }
                            ]
                        }
                    }
                },
                "minecraft:is_shaking": {}
            },
            "minecraft:strider_adult": {
                "minecraft:behavior.breed": {
                    "priority": 5,
                    "speed_multiplier": 1.0
                },
                "minecraft:breedable": {
                    "breed_items": [
                        "warped_fungus"
                    ],
                    "breeds_with": {
                        "baby_type": "minecraft:strider",
                        "breed_event": {
                            "event": "minecraft:entity_born",
                            "target": "baby"
                        },
                        "mate_type": "minecraft:strider"
                    },
                    "require_tame": false
                },
                "minecraft:experience_reward": {
                    "on_bred": "Math.Random(1,7)",
                    "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
                },
                "minecraft:loot": {
                    "table": "loot_tables/entities/strider.json"
                },
                "minecraft:type_family": {
                    "family": [
                        "strider",
                        "strider_adult",
                        "mob"
                    ]
                }
            },
            "minecraft:strider_baby": {
                "minecraft:ageable": {
                    "duration": 1200,
                    "feed_items": [
                        "warped_fungus"
                    ],
                    "grow_up": {
                        "event": "minecraft:ageable_grow_up",
                        "target": "self"
                    }
                },
                "minecraft:behavior.follow_parent": {
                    "priority": 9,
                    "speed_multiplier": 1.1
                },
                "minecraft:is_baby": {},
                "minecraft:scale": {
                    "value": 0.5
                },
                "minecraft:type_family": {
                    "family": [
                        "strider",
                        "strider_baby",
                        "mob"
                    ]
                }
            },
            "minecraft:strider_parent_jockey": {
                "minecraft:addrider": {
                    "entity_type": "minecraft:strider",
                    "spawn_event": "spawn_baby"
                },
                "minecraft:rideable": {
                    "family_types": [
                        "strider"
                    ],
                    "seat_count": 1,
                    "seats": {
                        "position": [
                            0.0,
                            1.6,
                            0.0
                        ]
                    }
                }
            },
            "minecraft:strider_piglin_jockey": {
                "minecraft:addrider": {
                    "entity_type": "minecraft:zombie_pigman",
                    "spawn_event": "minecraft:spawn_as_strider_jockey"
                },
                "minecraft:rideable": {
                    "crouching_skip_interact": true,
                    "family_types": [
                        "player",
                        "zombie_pigman"
                    ],
                    "interact_text": "action.interact.ride.strider",
                    "seat_count": 1,
                    "seats": {
                        "position": [
                            0.0,
                            1.65,
                            -0.2
                        ]
                    }
                }
            },
            "minecraft:strider_saddled": {
                "minecraft:behavior.controlled_by_player": {
                    "mount_speed_multiplier": 1.45,
                    "priority": 0
                },
                "minecraft:boostable": {
                    "boost_items": [
                        {
                            "damage": 1,
                            "item": "warped_fungus_on_a_stick",
                            "replace_item": "fishing_rod"
                        }
                    ],
                    "duration": 16.0,
                    "speed_multiplier": 2.0
                },
                "minecraft:is_saddled": {},
                "minecraft:item_controllable": {
                    "control_items": "warped_fungus_on_a_stick"
                },
                "minecraft:loot": {
                    "table": "loot_tables/entities/strider_saddled.json"
                },
                "minecraft:rideable": {
                    "crouching_skip_interact": true,
                    "family_types": [
                        "player"
                    ],
                    "interact_text": "action.interact.ride.strider",
                    "seat_count": 1,
                    "seats": {
                        "position": [
                            0.0,
                            1.6,
                            -0.2
                        ]
                    }
                }
            },
            "minecraft:strider_unsaddled": {
                "minecraft:interact": {
                    "interactions": [
                        {
                            "interact_text": "action.interact.saddle",
                            "on_interact": {
                                "event": "minecraft:on_saddled",
                                "filters": {
                                    "domain": "hand",
                                    "subject": "other",
                                    "test": "has_equipment",
                                    "value": "saddle"
                                }
                            },
                            "play_sounds": "saddle",
                            "use_item": true
                        }
                    ]
                }
            }
        },
        "components": {
            "minecraft:attack": {
                "damage": 0
            },
            "minecraft:balloonable": {},
            "minecraft:behavior.avoid_mob_type": {
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "parasaurScary"
                                },
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "carnivore"
                                }
                            ]
                        },
                        "max_dist": 12,
                        "must_see": true,
                        "sprint_speed_multiplier": 1.2,
                        "walk_speed_multiplier": 1.2
                    }
                ],
                "priority": 3
            },
            "minecraft:behavior.look_at_player": {
                "look_distance": 6.0,
                "priority": 12,
                "probability": 0.02
            },
            "minecraft:behavior.melee_attack": {
                "priority": 8
            },
            "minecraft:behavior.move_to_liquid": {
                "goal_radius": 0.9,
                "material_type": "Lava",
                "priority": 10,
                "search_count": 30,
                "search_height": 10,
                "search_range": 16
            },
            "minecraft:behavior.nearest_attackable_target": {
                "entity_types": [
                    {
                        "filters": {
                            "subject": "other",
                            "test": "is_family",
                            "value": "parasaurAngerer"
                        },
                        "max_dist": 64
                    }
                ],
                "priority": 7
            },
            "minecraft:behavior.panic": {
                "panic_sound": "panic",
                "priority": 4,
                "sound_interval": {
                    "range_max": 3.0,
                    "range_min": 1.0
                },
                "speed_multiplier": 1.1
            },
            "minecraft:behavior.random_look_around": {
                "priority": 13
            },
            "minecraft:behavior.random_stroll": {
                "priority": 11,
                "speed_multiplier": 0.8
            },
            "minecraft:behavior.rise_to_liquid_level": {
                "liquid_y_offset": 0.25,
                "priority": 0,
                "rise_delta": 0.01,
                "sink_delta": 0.01
            },
            "minecraft:behavior.tempt": {
                "can_tempt_while_ridden": true,
                "items": [
                    "warped_fungus",
                    "warped_fungus_on_a_stick"
                ],
                "priority": 6,
                "sound_interval": {
                    "range_max": 5.0,
                    "range_min": 2.0
                },
                "speed_multiplier": 1.2,
                "tempt_sound": "tempt"
            },
            "minecraft:collision_box": {
                "height": 1.7,
                "width": 0.9
            },
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:despawn": {
                "despawn_from_distance": {}
            },
            "minecraft:fire_immune": {},
            "minecraft:health": {
                "max": 20,
                "value": 20
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "cause": "drowning",
                        "damage_per_tick": 1,
                        "filters": {
                            "operator": "==",
                            "test": "in_contact_with_water",
                            "value": true
                        }
                    }
                ]
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:jump.static": {},
            "minecraft:lava_movement": {
                "value": 0.32
            },
            "minecraft:leashable": {
                "hard_distance": 6.0,
                "max_distance": 10.0,
                "soft_distance": 4.0
            },
            "minecraft:movement": {
                "value": 0.16
            },
            "minecraft:movement.basic": {},
            "minecraft:nameable": {},
            "minecraft:navigation.walk": {
                "avoid_water": true,
                "can_path_over_lava": true,
                "can_sink": false,
                "can_walk_in_lava": true
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:type_family": {
                "family": [
                    "strider",
                    "mob",
                    "animal"
                ]
            }
        },
        "description": {
            "animations": {
                "animBleed": "animation.general_bleed",
                "animBleedCountdown": "animation.general_bleed_countdown",
                "contBleed": "controller.animation.general_manage_bleed"
            },
            "identifier": "minecraft:strider",
            "is_experimental": false,
            "is_spawnable": true,
            "is_summonable": true,
            "properties": {
                "rift:bleeding": {
                    "client_sync": true,
                    "default": false,
                    "type": "bool"
                }
            },
            "scripts": {
                "animate": [
                    "contBleed"
                ]
            }
        },
        "events": {
            "minecraft:ageable_grow_up": {
                "add": {
                    "component_groups": [
                        "minecraft:strider_adult",
                        "minecraft:strider_unsaddled"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:strider_baby"
                    ]
                }
            },
            "minecraft:entity_born": {
                "trigger": "spawn_baby"
            },
            "minecraft:entity_spawned": {
                "randomize": [
                    {
                        "trigger": "spawn_adult",
                        "weight": 40
                    },
                    {
                        "trigger": "spawn_adult_piglin_jockey",
                        "weight": 2
                    },
                    {
                        "trigger": "spawn_adult_parent_jockey",
                        "weight": 8
                    },
                    {
                        "trigger": "spawn_baby",
                        "weight": 50
                    }
                ]
            },
            "minecraft:on_saddled": {
                "add": {
                    "component_groups": [
                        "minecraft:strider_saddled"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:strider_unsaddled"
                    ]
                }
            },
            "rift:start_bleeding": {
                "set_property": {
                    "rift:bleeding": true
                }
            },
            "rift:stop_bleeding": {
                "set_property": {
                    "rift:bleeding": false
                }
            },
            "spawn_adult": {
                "add": {
                    "component_groups": [
                        "minecraft:strider_adult",
                        "minecraft:strider_unsaddled",
                        "minecraft:detect_suffocating"
                    ]
                }
            },
            "spawn_adult_parent_jockey": {
                "add": {
                    "component_groups": [
                        "minecraft:strider_adult",
                        "minecraft:strider_parent_jockey",
                        "minecraft:strider_unsaddled",
                        "minecraft:detect_suffocating"
                    ]
                }
            },
            "spawn_adult_piglin_jockey": {
                "add": {
                    "component_groups": [
                        "minecraft:strider_adult",
                        "minecraft:strider_saddled",
                        "minecraft:strider_piglin_jockey",
                        "minecraft:detect_suffocating"
                    ]
                }
            },
            "spawn_baby": {
                "add": {
                    "component_groups": [
                        "minecraft:strider_baby",
                        "minecraft:detect_suffocating"
                    ]
                }
            },
            "start_suffocating": {
                "add": {
                    "component_groups": [
                        "minecraft:start_suffocating"
                    ]
                }
            },
            "stop_suffocating": {
                "add": {
                    "component_groups": [
                        "minecraft:detect_suffocating"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:start_suffocating"
                    ]
                }
            }
        }
    }
}