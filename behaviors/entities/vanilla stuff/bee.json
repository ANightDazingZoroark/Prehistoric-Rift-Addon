                },
                "remove": {
                    "component_groups": [
                        "collect_nectar",
                        "track_attacker",
                        "take_nearest_target",
                        "look_for_food",
                        "angry_bee",
                        "hive_full",
                        "find_hive",
                        "escape_fire"
                    ]
                }
            },
            "find_flower_timeout": {
                "add": {
                    "component_groups": [
                        "return_to_home"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "look_for_food"
                    ]
                }
            },
            "find_hive_event": {
                "add": {
                    "component_groups": [
                        "find_hive"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "return_to_home",
                        "hive_full"
                    ]
                }
            },
            "find_hive_timeout": {
                "sequence": [
                    {
                        "add": {
                            "component_groups": [
                                "look_for_food"
                            ]
                        },
                        "filters": {
                            "domain": "minecraft:has_nectar",
                            "operator": "!=",
                            "test": "bool_property"
                        },
                        "remove": {
                            "component_groups": [
                                "find_hive",
                                "escape_fire"
                            ]
                        }
                    },
                    {
                        "add": {
                            "component_groups": [
                                "return_to_home"
                            ]
                        },
                        "filters": {
                            "domain": "minecraft:has_nectar",
                            "test": "bool_property"
                        },
                        "remove": {
                            "component_groups": [
                                "find_hive"
                            ]
                        }
                    }
                ]
            },
            "hive_destroyed": {
                "sequence": [
                    {
                        "add": {
                            "component_groups": [
                                "take_nearest_target"
                            ]
                        },
                        "filters": {
                            "operator": "!=",
                            "subject": "self",
                            "test": "is_family",
                            "value": "pacified"
                        },
                        "remove": {
                            "component_groups": [
                                "escape_fire"
                            ]
                        }
                    }
                ]
            },
            "minecraft:ageable_grow_up": {
                "add": {
                    "component_groups": [
                        "bee_adult"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "bee_baby"
                    ]
                }
            },
            "minecraft:entity_born": {
                "add": {
                    "component_groups": [
                        "bee_baby",
                        "shelter_detection",
                        "track_attacker",
                        "look_for_food"
                    ]
                }
            },
            "minecraft:entity_spawned": {
                "sequence": [
                    {
                        "randomize": [
                            {
                                "add": {
                                    "component_groups": [
                                        "bee_adult"
                                    ]
                                },
                                "weight": 95
                            },
                            {
                                "add": {
                                    "component_groups": [
                                        "bee_baby"
                                    ]
                                },
                                "weight": 5
                            }
                        ]
                    },
                    {
                        "add": {
                            "component_groups": [
                                "track_attacker",
                                "shelter_detection",
                                "look_for_food"
                            ]
                        }
                    }
                ]
            },
            "minecraft:exited_disturbed_hive": {
