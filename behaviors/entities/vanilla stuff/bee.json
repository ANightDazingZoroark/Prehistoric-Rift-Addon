{
    "format_version": "1.18.20",
    "minecraft:entity": {
        "component_groups": {
            "abort_shelter_detection": {
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "event": "abort_sheltering",
                            "filters": {
                                "all_of": [
                                    {
                                        "operator": "==",
                                        "test": "weather",
                                        "value": "clear"
                                    },
                                    {
                                        "test": "is_daytime",
                                        "value": true
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            "angry_bee": {
                "minecraft:angry": {
                    "broadcastAnger": true,
                    "broadcastRange": 20,
                    "broadcast_filters": {
                        "operator": "!=",
                        "test": "is_family",
                        "value": "pacified"
                    },
                    "calm_event": {
                        "event": "calmed_down",
                        "target": "self"
                    },
                    "duration": 25
                },
                "minecraft:behavior.melee_attack": {
                    "attack_once": true,
                    "on_attack": {
                        "event": "countdown_to_perish_event",
                        "target": "self"
                    },
                    "priority": 2,
                    "speed_multiplier": 1.4
                }
            },
            "bee_adult": {
                "minecraft:behavior.breed": {
                    "priority": 4,
                    "speed_multiplier": 1.0
                },
                "minecraft:breedable": {
                    "breed_items": [
                        "minecraft:red_flower",
                        "minecraft:yellow_flower",
                        "minecraft:wither_rose",
                        "minecraft:double_plant:0",
                        "minecraft:double_plant:1",
                        "minecraft:double_plant:4",
                        "minecraft:double_plant:5",
                        "minecraft:flowering_azalea",
                        "minecraft:azalea_leaves_flowered",
                        "minecraft:mangrove_propagule"
                    ],
                    "breeds_with": {
                        "baby_type": "minecraft:bee",
                        "breed_event": {
                            "event": "minecraft:entity_born",
                            "target": "baby"
                        },
                        "mate_type": "minecraft:bee"
                    },
                    "require_tame": false
                },
                "minecraft:experience_reward": {
                    "on_bred": "Math.Random(1,7)",
                    "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
                }
            },
            "bee_baby": {
                "minecraft:ageable": {
                    "duration": 1200,
                    "feed_items": [
                        "minecraft:red_flower",
                        "minecraft:yellow_flower",
                        "minecraft:wither_rose",
                        "minecraft:double_plant:0",
                        "minecraft:double_plant:1",
                        "minecraft:double_plant:4",
                        "minecraft:double_plant:5",
                        "minecraft:flowering_azalea",
                        "minecraft:azalea_leaves_flowered",
                        "minecraft:mangrove_propagule"
                    ],
                    "grow_up": {
                        "event": "minecraft:ageable_grow_up",
                        "target": "self"
                    }
                },
                "minecraft:behavior.follow_parent": {
                    "priority": 11,
                    "speed_multiplier": 1.1
                },
                "minecraft:is_baby": {},
                "minecraft:scale": {
                    "value": 0.5
                }
            },
            "countdown_to_perish": {
                "minecraft:behavior.panic": {
                    "force": true,
                    "priority": 1,
                    "speed_multiplier": 1.25
                },
                "minecraft:mark_variant": {
                    "value": 1
                },
                "minecraft:timer": {
                    "looping": false,
                    "randomInterval": true,
                    "time": [
                        10,
                        60
                    ],
                    "time_down_event": {
                        "event": "perish_event",
                        "target": "self"
                    }
                },
                "minecraft:type_family": {
                    "family": [
                        "bee",
                        "mob",
                        "arthropod",
                        "pacified",
                        "animal"
                    ]
                }
            },
            "default_sound": {
                "minecraft:ambient_sound_interval": {
                    "event_name": "ambient",
                    "range": 0.0,
                    "value": 0.0
                }
            },
            "easy_attack": {
                "minecraft:attack": {
                    "damage": 2
                }
            },
            "escape_fire": {
                "minecraft:behavior.panic": {
                    "force": true,
                    "priority": 1,
                    "speed_multiplier": 1.25
                },
                "minecraft:timer": {
                    "looping": false,
                    "randomInterval": true,
                    "time": [
                        20,
                        50
                    ],
                    "time_down_event": {
                        "event": "stop_panicking_after_fire",
                        "target": "self"
                    }
                }
            },
            "find_hive": {
                "minecraft:behavior.move_to_block": {
                    "goal_radius": 0.633,
                    "on_reach": [
                        {
                            "event": "minecraft:bee_returned_to_hive",
                            "target": "block"
                        }
                    ],
                    "priority": 10,
                    "search_height": 10,
                    "search_range": 16,
                    "target_blocks": [
                        "bee_nest",
                        "beehive"
                    ],
                    "tick_interval": 1
                },
                "minecraft:timer": {
                    "looping": false,
                    "time": 180,
                    "time_down_event": {
                        "event": "find_hive_timeout",
                        "target": "self"
                    }
                }
            },
            "hard_attack": {
                "minecraft:attack": {
                    "damage": 2,
                    "effect_duration": 18,
                    "effect_name": "poison"
                }
            },
            "has_nectar": {
                "minecraft:grows_crop": {
                    "chance": 0.03,
                    "charges": 10
                }
            },
            "hive_full": {
                "minecraft:timer": {
                    "looping": false,
                    "randomInterval": true,
                    "time": [
                        5,
                        20
                    ],
                    "time_down_event": {
                        "event": "find_hive_event",
                        "target": "self"
                    }
                }
            },
            "look_for_food": {
                "minecraft:ambient_sound_interval": {
                    "event_name": "ambient.pollinate",
                    "range": 3.0,
                    "value": 2.0
                },
                "minecraft:behavior.move_to_block": {
                    "goal_radius": 1.0,
                    "on_stay_completed": [
                        {
                            "event": "collected_nectar",
                            "target": "self"
                        }
                    ],
                    "priority": 10,
                    "search_height": 4,
                    "search_range": 6,
                    "start_chance": 0.5,
                    "stay_duration": 20.0,
                    "target_block_filters": {
                        "operator": "==",
                        "subject": "block",
                        "test": "is_waterlogged",
                        "value": false
                    },
                    "target_blocks": [
                        "minecraft:red_flower",
                        "minecraft:yellow_flower",
                        "minecraft:wither_rose",
                        "minecraft:double_plant:8",
                        "minecraft:double_plant:9",
                        "minecraft:double_plant:12",
                        "minecraft:double_plant:13",
                        "minecraft:flowering_azalea",
                        "minecraft:azalea_leaves_flowered",
                        "minecraft:mangrove_propagule"
                    ],
                    "target_offset": [
                        0,
                        0.25,
                        0
                    ],
                    "target_selection_method": "random",
                    "tick_interval": 1
                },
                "minecraft:timer": {
                    "looping": true,
                    "time": 180,
                    "time_down_event": {
                        "event": "find_flower_timeout"
                    }
                }
            },
            "normal_attack": {
                "minecraft:attack": {
                    "damage": 2,
                    "effect_duration": 10,
                    "effect_name": "poison"
                }
            },
            "perish": {
                "minecraft:hurt_on_condition": {
                    "damage_conditions": [
                        {
                            "cause": "none",
                            "damage_per_tick": 999
                        }
                    ]
                }
            },
            "return_to_home": {
                "minecraft:behavior.go_home": {
                    "goal_radius": 1.2,
                    "interval": 1,
                    "on_failed": [
                        {
                            "event": "find_hive_event",
                            "target": "self"
                        }
                    ],
                    "on_home": [
                        {
                            "event": "minecraft:bee_returned_to_hive",
                            "filters": {
                                "any_of": [
                                    {
                                        "subject": "block",
                                        "test": "is_block",
                                        "value": "minecraft:bee_nest"
                                    },
                                    {
                                        "subject": "block",
                                        "test": "is_block",
                                        "value": "minecraft:beehive"
                                    }
                                ]
                            },
                            "target": "block"
                        },
                        {
                            "event": "find_hive_event",
                            "filters": {
                                "all_of": [
                                    {
                                        "operator": "!=",
                                        "subject": "block",
                                        "test": "is_block",
                                        "value": "minecraft:bee_nest"
                                    },
                                    {
                                        "operator": "!=",
                                        "subject": "block",
                                        "test": "is_block",
                                        "value": "minecraft:beehive"
                                    }
                                ]
                            },
                            "target": "self"
                        }
                    ],
                    "priority": 4,
                    "speed_multiplier": 1.0
                }
            },
            "shelter_detection": {
                "minecraft:environment_sensor": {
                    "triggers": [
                        {
                            "event": "seek_shelter",
                            "filters": {
                                "all_of": [
                                    {
                                        "any_of": [
                                            {
                                                "test": "is_daytime",
                                                "value": false
                                            },
                                            {
                                                "operator": "==",
                                                "test": "weather",
                                                "value": "precipitation"
                                            }
                                        ]
                                    },
                                    {
                                        "domain": "minecraft:has_nectar",
                                        "operator": "!=",
                                        "test": "bool_property"
                                    },
                                    {
                                        "test": "has_biome_tag",
                                        "value": "overworld"
                                    }
                                ]
                            }
                        }
                    ]
                }
            },
            "take_nearest_target": {
                "minecraft:behavior.nearest_attackable_target": {
                    "entity_types": [
                        {
                            "filters": {
                                "subject": "other",
                                "test": "is_family",
                                "value": "player"
                            },
                            "max_dist": 10
                        }
                    ],
                    "priority": 2
                },
                "minecraft:timer": {
                    "looping": true,
                    "time": 5,
                    "time_down_event": {
                        "event": "calmed_down",
                        "target": "self"
                    }
                }
            },
            "track_attacker": {
                "minecraft:behavior.hurt_by_target": {
                    "priority": 1
                }
            }
        },
        "components": {
            "minecraft:balloonable": {
                "mass": 0.5
            },
            "minecraft:behavior.float": {
                "priority": 19
            },
            "minecraft:behavior.move_towards_home_restriction": {
                "priority": 9
            },
            "minecraft:behavior.random_hover": {
                "hover_height": [
                    1,
                    4
                ],
                "interval": 1,
                "priority": 12,
                "xz_dist": 8,
                "y_dist": 8,
                "y_offset": -1
            },
            "minecraft:behavior.tempt": {
                "can_tempt_vertically": true,
                "items": [
                    "minecraft:red_flower",
                    "minecraft:yellow_flower",
                    "minecraft:wither_rose",
                    "minecraft:double_plant:0",
                    "minecraft:double_plant:1",
                    "minecraft:double_plant:4",
                    "minecraft:double_plant:5",
                    "minecraft:flowering_azalea",
