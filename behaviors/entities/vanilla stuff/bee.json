            "minecraft:on_target_acquired": {
                "event": "attacked",
                "target": "self"
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:type_family": {
                "family": [
                    "bee",
                    "mob",
                    "arthropod",
                    "animal"
                ]
            }
        },
        "description": {
            "animations": {
                "animBleed": "animation.general_bleed",
                "animBleedCountdown": "animation.general_bleed_countdown",
                "contBleed": "controller.animation.general_manage_bleed"
            },
            "identifier": "minecraft:bee",
            "is_experimental": false,
            "is_spawnable": true,
            "is_summonable": true,
            "properties": {
                "minecraft:has_nectar": {
                    "client_sync": true,
                    "default": "query.had_component_group('has_nectar')",
                    "type": "bool"
                },
                "rift:bleeding": {
                    "client_sync": true,
                    "default": false,
                    "type": "bool"
                }
            },
            "scripts": {
                "animate": [
                    "contBleed"
                ]
            }
        },
        "events": {
            "abort_sheltering": {
                "add": {
                    "component_groups": [
                        "shelter_detection",
                        "look_for_food"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "abort_shelter_detection",
                        "return_to_home",
                        "escape_fire"
                    ]
                }
            },
            "attacked": {
                "sequence": [
                    {
                        "add": {
                            "component_groups": [
                                "angry_bee"
                            ]
                        },
                        "remove": {
                            "component_groups": [
                                "take_nearest_target"
                            ]
                        }
                    },
                    {
                        "add": {
                            "component_groups": [
                                "easy_attack"
                            ]
                        },
                        "filters": {
                            "test": "is_difficulty",
