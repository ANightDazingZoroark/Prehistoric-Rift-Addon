                    ]
                }
            },
            "minecraft:hive_full": {
                "add": {
                    "component_groups": [
                        "hive_full"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "find_hive",
                        "return_to_home"
                    ]
                }
            },
            "perish_event": {
