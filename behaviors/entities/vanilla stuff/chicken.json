{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "component_groups": {
            "minecraft:chicken_adult": {
                "minecraft:behavior.breed": {
                    "priority": 6,
                    "speed_multiplier": 1.0
                },
                "minecraft:breedable": {
                    "breed_items": [
                        "wheat_seeds",
                        "beetroot_seeds",
                        "melon_seeds",
                        "pumpkin_seeds"
                    ],
                    "breeds_with": {
                        "baby_type": "minecraft:chicken",
                        "breed_event": {
                            "event": "minecraft:entity_born",
                            "target": "baby"
                        },
                        "mate_type": "minecraft:chicken"
                    },
                    "require_tame": false
                },
                "minecraft:experience_reward": {
                    "on_bred": "Math.Random(1,7)",
                    "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
                },
                "minecraft:loot": {
                    "table": "loot_tables/entities/chicken.json"
                },
                "minecraft:rideable": {
                    "family_types": [
                        "zombie"
                    ],
                    "seat_count": 1,
                    "seats": {
                        "position": [
                            0.0,
                            0.4,
                            0.0
                        ]
                    }
                },
                "minecraft:spawn_entity": {
                    "entities": {
                        "filters": {
                            "operator": "==",
                            "subject": "self",
                            "test": "rider_count",
                            "value": 0
                        },
                        "max_wait_time": 600,
                        "min_wait_time": 300,
                        "spawn_item": "egg",
                        "spawn_sound": "plop"
                    }
                }
            },
            "minecraft:chicken_baby": {
                "minecraft:ageable": {
                    "duration": 1200,
                    "feed_items": [
                        "wheat_seeds",
                        "beetroot_seeds",
                        "melon_seeds",
                        "pumpkin_seeds"
                    ],
                    "grow_up": {
                        "event": "minecraft:ageable_grow_up",
                        "target": "self"
                    }
                },
                "minecraft:behavior.follow_parent": {
                    "priority": 8,
                    "speed_multiplier": 1.1
                },
                "minecraft:is_baby": {},
                "minecraft:scale": {
                    "value": 0.5
                }
            }
        },
        "components": {
            "minecraft:attack": {
                "damage": 0
            },
            "minecraft:balloonable": {
                "mass": 0.5
            },
            "minecraft:behavior.avoid_mob_type": {
                "entity_types": [
                    {
                        "filters": {
                            "any_of": [
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "parasaurScary"
                                },
                                {
                                    "subject": "other",
                                    "test": "is_family",
                                    "value": "carnivore"
                                }
                            ]
                        },
                        "max_dist": 12,
                        "must_see": true,
                        "sprint_speed_multiplier": 1.2,
                        "walk_speed_multiplier": 1.2
                    }
                ],
                "priority": 1
            },
            "minecraft:behavior.float": {
                "priority": 0
            },
            "minecraft:behavior.look_at_player": {
                "look_distance": 6.0,
                "priority": 10,
                "probability": 0.02
            },
            "minecraft:behavior.melee_attack": {
                "priority": 4
            },
            "minecraft:behavior.mount_pathing": {
                "priority": 5,
                "speed_multiplier": 1.5,
                "target_dist": 0.0,
                "track_target": true
            },
            "minecraft:behavior.nearest_attackable_target": {
                "entity_types": [
                    {
                        "filters": {
                            "subject": "other",
                            "test": "is_family",
                            "value": "parasaurAngerer"
                        },
                        "max_dist": 64
                    }
                ],
                "priority": 3
            },
            "minecraft:behavior.panic": {
                "priority": 2,
                "speed_multiplier": 1.5
            },
            "minecraft:behavior.random_look_around": {
                "priority": 11
            },
            "minecraft:behavior.random_stroll": {
                "priority": 9,
                "speed_multiplier": 1.0
            },
            "minecraft:behavior.tempt": {
                "items": [
                    "wheat_seeds",
                    "beetroot_seeds",
                    "melon_seeds",
                    "pumpkin_seeds"
                ],
                "priority": 7,
                "speed_multiplier": 1.0
            },
            "minecraft:breathable": {
                "suffocate_time": 0,
                "total_supply": 15
            },
            "minecraft:can_climb": {},
            "minecraft:collision_box": {
                "height": 0.8,
                "width": 0.6
            },
            "minecraft:conditional_bandwidth_optimization": {},
            "minecraft:damage_sensor": {
                "triggers": {
                    "cause": "fall",
                    "deals_damage": false
                }
            },
            "minecraft:despawn": {
                "despawn_from_distance": {}
            },
            "minecraft:health": {
                "max": 4,
                "value": 4
            },
            "minecraft:hurt_on_condition": {
                "damage_conditions": [
                    {
                        "cause": "lava",
                        "damage_per_tick": 4,
                        "filters": {
                            "operator": "==",
                            "subject": "self",
                            "test": "in_lava",
                            "value": true
                        }
                    }
                ]
            },
            "minecraft:is_hidden_when_invisible": {},
            "minecraft:jump.static": {},
            "minecraft:leashable": {
                "hard_distance": 6.0,
                "max_distance": 10.0,
                "soft_distance": 4.0
            },
            "minecraft:movement": {
                "value": 0.25
            },
            "minecraft:movement.basic": {},
            "minecraft:nameable": {},
            "minecraft:navigation.walk": {
                "avoid_damage_blocks": true,
                "can_path_over_water": true
            },
            "minecraft:physics": {},
            "minecraft:pushable": {
                "is_pushable": true,
                "is_pushable_by_piston": true
            },
            "minecraft:type_family": {
                "family": [
                    "chicken",
                    "mob",
                    "animal"
                ]
            }
        },
        "description": {
            "animations": {
                "animBleed": "animation.general_bleed",
                "animBleedCountdown": "animation.general_bleed_countdown",
                "contBleed": "controller.animation.general_manage_bleed",
                "contMovingTag": "controller.animation.add_moving_tag"
            },
            "identifier": "minecraft:chicken",
            "is_experimental": false,
            "is_spawnable": true,
            "is_summonable": true,
            "properties": {
                "rift:bleeding": {
                    "client_sync": true,
                    "default": false,
                    "type": "bool"
                }
            },
            "scripts": {
                "animate": [
                    "contBleed",
                    "contMovingTag"
                ]
            }
        },
        "events": {
            "from_egg": {
                "add": {
                    "component_groups": [
                        "minecraft:chicken_baby"
                    ]
                }
            },
            "minecraft:ageable_grow_up": {
                "add": {
                    "component_groups": [
                        "minecraft:chicken_adult"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "minecraft:chicken_baby"
                    ]
                }
            },
            "minecraft:entity_born": {
                "add": {
                    "component_groups": [
                        "minecraft:chicken_baby"
                    ]
                },
                "remove": {}
            },
            "minecraft:entity_spawned": {
                "randomize": [
                    {
                        "trigger": "minecraft:spawn_adult",
                        "weight": 95
                    },
                    {
                        "add": {
                            "component_groups": [
                                "minecraft:chicken_baby"
                            ]
                        },
                        "weight": 5
                    }
                ]
            },
            "minecraft:spawn_adult": {
                "add": {
                    "component_groups": [
                        "minecraft:chicken_adult"
                    ]
                }
            },
            "rift:start_bleeding": {
                "set_property": {
                    "rift:bleeding": true
                }
            },
            "rift:stop_bleeding": {
                "set_property": {
                    "rift:bleeding": false
                }
            }
        }
    }
}